{
  "Routes": [
    {
      "UseServiceDiscovery": true,
      "DownstreamPathTemplate": "/api/v1/{everything}",
      "DownstreamScheme": "https",
      "ServiceName": "ApplicationService",
      "UpstreamPathTemplate": "/v1/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "ReRouteCaseSensitive": false,
      "SwaggerKey": "ApplicationService"
    },
    {
      "UseServiceDiscovery": true,
      "DownstreamPathTemplate": "/api/v2/{everything}",
      "DownstreamScheme": "https",
      "ServiceName": "ApplicationService",
      "UpstreamPathTemplate": "/v2/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "ReRouteCaseSensitive": false,
      "SwaggerKey": "ApplicationService"
    }
  ],
  "SwaggerEndpoints": [
    {
      "Config": [
        {
          "Name": "Application API",
          "Version": "v1",
          "Service": {
            "Name": "ApplicationService",
            "Path": "/swagger/v1/swagger.json"
          }
        },
        {
          "Name": "Application API",
          "Version": "v2",
          "Service": {
            "Name": "ApplicationService",
            "Path": "/swagger/v2/swagger.json"
          }
        }
      ],
      "Key": "ApplicationService",
      "TakeServersFromDownstreamService": true
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Scheme": "http",
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    }
  }
}
